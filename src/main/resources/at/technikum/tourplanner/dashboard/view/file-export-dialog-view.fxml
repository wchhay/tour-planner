<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<HBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="at.technikum.tourplanner.dashboard.view.FileExportDialogController" alignment="CENTER">
    <Button onAction="#export" layoutX="250.0" layoutY="30.0" mnemonicParsing="false" text="Export/Save File">
        <HBox.margin>
            <Insets topRightBottomLeft="4.0"/>
        </HBox.margin>
    </Button>
    <Button onAction="#closeDialog" layoutX="250.0" layoutY="30.0" mnemonicParsing="false" text="Cancel">
        <HBox.margin>
            <Insets topRightBottomLeft="4.0"/>
        </HBox.margin>
    </Button>
</HBox>
